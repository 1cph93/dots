[bar/test]
monitor = eDP-1
width = 100%
height = 30
offset_x = 0
offset_y = 0
padding_left = 2
padding_right = 2
bottom = true
dock = false
lineheight = 10
background = #aaff184f
foreground = #fff
linecolor = #c51850
font-0 = envypn:pixelsize=13;0

modules-left = net-wireless
modules-right = net-wired

[module/net-wireless]
type = internal/network
interface = net1
interval = 3.0

label-connected = %ifname%: Connected to %essid% with ip %local_ip% (signal: %signal%) - D %downspeed% U %upspeed%
label-disconnected = %ifname%: Not connected

[module/net-wired]
type = internal/network
interface = net0
interval = 3.0

label-connected = %ifname%: Connected with ip %local_ip%
label-disconnected = %ifname%: Not connected

; vim:ft=dosini
