[bar/test]
monitor = eDP-1
width = 300
height = 50
dock = true
lineheight = 10
background = #ff184f
foreground = #671414
linecolor = #c51850
;offset-x = 850
;offset-y = 450
font-0 = envypn:pixelsize=13;0

modules-center = volume


[module/volume]
type = internal/volume
mapped = true
speaker-mixer = Speaker
headphone-mixer = Headphone
headphone-id = 9

format-volume = <label-volume>
label-volume = %percentage%
label-muted = muted

; vim:ft=dosini
