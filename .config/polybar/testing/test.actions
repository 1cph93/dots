[bar/test]
monitor = eDP-1
width = 100%
height = 30
dock = true
separator = " | "
lineheight = 10
background = #ff184f
foreground = #000000
linecolor = #c51850
font-0 = Unifont:size=15;0

border-left = 5
border-left-color = #3f0079
border-right = 5
border-right-color = #3f0079

; modules-left = test
modules-left = button-index-fallback
modules-center = test
; modules-right = test test2

[module/button-index-fallback]
type = custom/text
content = %{A:notify-send -u low test:}foo%{A}

[module/test]
type = custom/text
content = ████

click-left = echo left
; click-middle = echo middle
; click-right = echo right
; scroll-up = echo scroll up
; scroll-down = echo scroll down

[module/test2]
type = custom/text
content = xxx

; vim:ft=dosini
